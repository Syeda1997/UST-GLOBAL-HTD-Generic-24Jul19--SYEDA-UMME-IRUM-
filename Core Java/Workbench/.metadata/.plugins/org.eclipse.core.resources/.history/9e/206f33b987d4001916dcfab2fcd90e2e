public class Test {

	public static void main(String[] args) {
		int i=98;
		char b=(char) i; //casting
		System.out.println(b);
		
		Scanner sc=new Scanner(System.in);
		int i;
		System.out.println("Enter the Integer");
		i=sc.nextInt();
		
	}

}
